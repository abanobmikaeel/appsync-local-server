{
  "name": "appsync-local-server",
  "version": "0.0.2",
  "description": "A tool for running and testing appsync javascript resolvers locally ",
  "keywords": [
    "appsync",
    "local",
    "server",
    "graphql"
  ],
  "license": "MIT",
  "author": "Abanob Mikaeel",
  "type": "module",
  "main": "index.js",
  "bin": {
    "appsync-local": "./bin/appsync-local.js"
  },
  "scripts": {
    "prepare": "echo \"No build needed\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit"
  },
  "devDependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@types/jest": "^29.5.8",
    "apollo-server-express": "^3.0.0",
    "express": "^4.18.2",
    "graphql": "^16.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "yargs": "^17.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.812.0",
    "@aws-sdk/lib-dynamodb": "^3.812.0",
    "@graphql-tools/graphql-file-loader": "^8.0.19",
    "@graphql-tools/load": "^8.1.0",
    "@graphql-tools/schema": "^8.2.0",
    "acorn": "^8.15.0",
    "apollo-server-express": "^3.0.0",
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "zod": "^4.0.14"
  }
}
